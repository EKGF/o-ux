}, {
  "@id" : "https://ekgf.github.io/o-ux/component#NavigationDrawer",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://purl.org/dc/elements/1.1/creator" : [ {
    "@id" : "http://orcid.org/0000-0003-2309-5068"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Navigation drawers provide access to destinations in your app.\n\nNavigation drawers provide access to destinations and app functionality, such as switching accounts. They can either be permanently on-screen or controlled by a navigation menu icon.\n\nNavigation drawers are recommended for:\n\n- Apps with five or more top-level destinations\n- Apps with two or more levels of navigation hierarchy\n- Quick navigation between unrelated destinations"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Navigation Drawer"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#seeAlso" : [ {
    "@id" : "https://material.io/components/navigation-drawer"
  } ],
