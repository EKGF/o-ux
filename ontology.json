}, {
  "@id" : "https://ekgf.github.io/o-ux/component#UseCases",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://purl.org/dc/elements/1.1/creator" : [ {
    "@id" : "http://orcid.org/0000-0003-2309-5068"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The UseCases component collects all current use cases from the EKG that the current User could have access to given the current LandingThing.\n\nFor each supported Use Case it knows the EKG/IRI, its label and the Personas that the current user has.\n\nOther components can ask the UseCases-component for more details\nabout whether a given Story in a given UseCase is supported for the\ngiven Persona.\nThat information could then be used to show or not show certain elements\nin the user interface that may or may not be available to the current user."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Use Cases"
  } ],
